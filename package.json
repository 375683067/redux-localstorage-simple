  "description": "Save and load Redux state to and from LocalStorage. Supports Immutable.js data structures.",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && babel src --out-dir dist",
    "test": "tape -r babel-register test/tape/**/*.js | tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kilkelly/redux-localstorage-simple.git"
  },
  "keywords": [
    "redux",
    "localstorage",
    "immutablejs"
  ],
  "author": "Frank Kilkelly",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kilkelly/redux-localstorage-simple/issues"
  },
  "homepage": "https://github.com/kilkelly/redux-localstorage-simple#readme",
  "dependencies": {
    "immutable": "^3.8.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "rimraf": "^2.5.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.1"
  }
}
